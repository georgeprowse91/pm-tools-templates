{
  "name": "pm-tools-templates-site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "test:e2e": "playwright test",
    "ci": "node server.js & echo $! > .server.pid && npx wait-on http://localhost:5173 && playwright test && lhci autorun --collect.url=http://localhost:5173 && kill $(cat .server.pid) || (kill $(cat .server.pid) || true; exit 1)"
  },
  "dependencies": {
    "express": "^5.1.0",
    "marked": "^16.2.0",
    "highlight.js": "^11.9.0",
    "morgan": "^1.10.0",
    "etag": "^1.8.1",
    "express-rate-limit": "^8.0.1"
  },
  "overrides": {
    "ws": "^8.17.1",
    "xlsx": "^0.18.5",
    "tar-fs": "^3.0.6",
    "nth-check": "^2.1.1",
    "cookie": "^0.7.0",
    "on-headers": "^1.0.2",
    "webpack-dev-server": "^4.15.2",
    "esbuild": "^0.25.0",
    "form-data": "^4.0.0",
    "tmp": "^0.2.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@playwright/test": "^1.55.0",
    "lighthouse": "^12.1.0",
    "@lhci/cli": "^0.15.1",
    "wait-on": "^8.0.4"
  }
}
