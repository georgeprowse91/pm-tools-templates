# Copy to .env for local development only. Do not commit real secrets.
# Jira (Atlassian)
JIRA_CLIENT_ID=
JIRA_CLIENT_SECRET=
JIRA_CLOUD_SCOPES=read:jira-user,read:jira-work,write:jira-work,manage:jira-configuration
JIRA_REDIRECT_URI=http://localhost:5173/oauth/callback/jira

# Asana
ASANA_CLIENT_ID=
ASANA_CLIENT_SECRET=
ASANA_SCOPES=default
ASANA_REDIRECT_URI=http://localhost:5173/oauth/callback/asana

# General
NODE_ENV=development
LOG_LEVEL=info
DRY_RUN=true
