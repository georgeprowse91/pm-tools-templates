  "tasks": [
    {
      "id": "sprint-001",
      "name": "Sprint Planning Meeting - Part 1",
      "description": "Review and select user stories from the product backlog for the upcoming sprint",
      "estimatedHours": 4,
      "dependencies": [],
      "assigneeRole": "Scrum Master",
      "priority": "Critical",
      "tags": ["sprint-planning", "backlog", "user-stories"],
      "customFields": {
        "sprint_status": "Planning",
        "meeting_type": "Sprint Planning",
        "timeboxed_hours": 4
      },
      "subtasks": [
        {
          "id": "sprint-001-1",
          "name": "Review sprint goal",
          "description": "Collaborate with Product Owner to define clear sprint goal",
          "estimatedHours": 0.5,
          "assigneeRole": "Product Owner",
          "priority": "Critical"
        },
        {
          "id": "sprint-001-2",
          "name": "Review product backlog",
          "description": "Review prioritized user stories in the product backlog",
          "estimatedHours": 1,
          "assigneeRole": "Product Owner",
          "priority": "High"
        },
        {
          "id": "sprint-001-3",
          "name": "Estimate story points",
          "description": "Collaborate with development team to estimate story points",
          "estimatedHours": 1.5,
          "assigneeRole": "Development Team",
          "priority": "High"
        },
        {
          "id": "sprint-001-4",
          "name": "Select stories for sprint",
          "description": "Choose user stories that fit within team capacity",
          "estimatedHours": 1,
          "assigneeRole": "Development Team",
          "priority": "High"
        }
      ]
    },
    {
      "id": "sprint-002",
      "name": "Sprint Planning Meeting - Part 2",
      "description": "Break down selected user stories into specific tasks and plan the sprint work",
      "estimatedHours": 4,
      "dependencies": ["sprint-001"],
      "assigneeRole": "Development Team",
      "priority": "Critical",
      "tags": ["sprint-planning", "task-breakdown", "estimation"],
      "customFields": {
        "sprint_status": "Planning",
        "meeting_type": "Sprint Planning",
        "timeboxed_hours": 4
      },
      "subtasks": [
        {
          "id": "sprint-002-1",
          "name": "Break down user stories",
          "description": "Decompose selected user stories into specific development tasks",
          "estimatedHours": 2,
          "assigneeRole": "Development Team",
          "priority": "Critical"
        },
        {
          "id": "sprint-002-2",
          "name": "Estimate task effort",
          "description": "Estimate time required for each development task",
          "estimatedHours": 1,
          "assigneeRole": "Development Team",
          "priority": "High"
        },
        {
          "id": "sprint-002-3",
          "name": "Assign tasks to team members",
          "description": "Distribute tasks among development team members",
          "estimatedHours": 0.5,
          "assigneeRole": "Scrum Master",
          "priority": "High"
        },
        {
          "id": "sprint-002-4",
          "name": "Finalize sprint backlog",
          "description": "Create final sprint backlog with all tasks and assignments",
          "estimatedHours": 0.5,
          "assigneeRole": "Scrum Master",
          "priority": "High"
        }
      ]
    },
    {
      "id": "sprint-003",
      "name": "Set Up Sprint Board",
      "description": "Configure and organize the sprint task board with appropriate columns and workflows",
      "estimatedHours": 2,
      "dependencies": ["sprint-002"],
      "assigneeRole": "Scrum Master",
      "priority": "High",
      "tags": ["sprint-board", "kanban", "workflow"],
      "customFields": {
        "sprint_status": "Planning",
        "board_type": "Kanban",
        "workflow_states": "To Do, In Progress, Review, Done"
      },
      "subtasks": [
        {
          "id": "sprint-003-1",
          "name": "Create sprint board columns",
          "description": "Set up To Do, In Progress, Review, and Done columns",
          "estimatedHours": 0.5,
          "assigneeRole": "Scrum Master",
          "priority": "High"
        },
        {
          "id": "sprint-003-2",
          "name": "Add user stories and tasks",
          "description": "Populate sprint board with selected user stories and tasks",
          "estimatedHours": 1,
          "assigneeRole": "Scrum Master",
          "priority": "High"
        },
        {
          "id": "sprint-003-3",
          "name": "Configure workflow rules",
          "description": "Set up board rules and automation for task transitions",
          "estimatedHours": 0.5,
          "assigneeRole": "Scrum Master",
          "priority": "Medium"
        }
      ]
    },
    {
      "id": "sprint-004",
      "name": "Daily Standup Planning",
      "description": "Schedule and prepare for daily standup meetings throughout the sprint",
      "estimatedHours": 2,
      "dependencies": ["sprint-003"],
      "assigneeRole": "Scrum Master",
      "priority": "High",
      "tags": ["daily-standup", "scheduling", "communication"],
      "customFields": {
        "sprint_status": "Planning",
        "meeting_frequency": "Daily",
        "meeting_duration": "15 minutes"
      },
      "subtasks": [
        {
          "id": "sprint-004-1",
          "name": "Schedule daily standups",
          "description": "Set up recurring calendar invites for daily standup meetings",
          "estimatedHours": 0.5,
          "assigneeRole": "Scrum Master",
          "priority": "High"
        },
        {
          "id": "sprint-004-2",
          "name": "Prepare standup format",
          "description": "Define structure and questions for daily standup meetings",
          "estimatedHours": 0.5,
          "assigneeRole": "Scrum Master",
          "priority": "Medium"
        },
        {
          "id": "sprint-004-3",
          "name": "Set up impediment tracking",
          "description": "Create system for tracking and resolving impediments",
          "estimatedHours": 1,
          "assigneeRole": "Scrum Master",
          "priority": "Medium"
        }
      ]
    },
    {
      "id": "sprint-005",
      "name": "Sprint Review Preparation",
      "description": "Plan and prepare for the sprint review/demo session",
      "estimatedHours": 3,
      "dependencies": ["sprint-002"],
      "assigneeRole": "Product Owner",
      "priority": "Medium",
      "tags": ["sprint-review", "demo", "stakeholders"],
