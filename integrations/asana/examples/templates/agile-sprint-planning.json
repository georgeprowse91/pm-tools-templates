      "subtasks": [
        {
          "id": "sprint-008-1",
          "name": "Check team availability",
          "description": "Verify team member availability during sprint period",
          "estimatedHours": 0.5,
          "assigneeRole": "Scrum Master",
          "priority": "High"
        },
        {
          "id": "sprint-008-2",
          "name": "Calculate working hours",
          "description": "Calculate total available working hours for the sprint",
          "estimatedHours": 0.5,
          "assigneeRole": "Scrum Master",
          "priority": "High"
        },
        {
          "id": "sprint-008-3",
