        {
          "id": "sprint-005-2",
          "name": "Schedule sprint review",
          "description": "Set up calendar invite for sprint review meeting",
          "estimatedHours": 0.5,
          "assigneeRole": "Product Owner",
          "priority": "Medium"
        },
        {
          "id": "sprint-005-3",
          "name": "Prepare demo environment",
          "description": "Ensure demo environment is ready for sprint review",
          "estimatedHours": 1,
          "assigneeRole": "Development Team",
          "priority": "Medium"
        },
        {
          "id": "sprint-005-4",
          "name": "Plan demo script",
          "description": "Outline what will be demonstrated in the sprint review",
          "estimatedHours": 1,
          "assigneeRole": "Product Owner",
          "priority": "Low"
        }
      ]
    },
