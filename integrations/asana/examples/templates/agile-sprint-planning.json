        {
          "id": "sprint-007-1",
          "name": "Review current DoD",
          "description": "Review existing Definition of Done criteria with team",
          "estimatedHours": 0.5,
          "assigneeRole": "Scrum Master",
          "priority": "High"
        },
        {
          "id": "sprint-007-2",
          "name": "Update DoD if needed",
          "description": "Make any necessary updates to Definition of Done",
          "estimatedHours": 0.5,
          "assigneeRole": "Development Team",
          "priority": "Medium"
        }
