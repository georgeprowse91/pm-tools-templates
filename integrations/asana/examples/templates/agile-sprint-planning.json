    {
      "name": "product_owner",
      "type": "user",
      "required": true,
      "defaultValue": null
    },
    {
      "name": "sprint_start_date",
      "type": "date",
      "required": true,
      "defaultValue": null
    },
    {
      "name": "sprint_end_date",
      "type": "date",
      "required": true,
      "defaultValue": null
