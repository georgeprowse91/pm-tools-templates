# Development Environment Configuration
NODE_ENV=development
REACT_APP_ENVIRONMENT=development

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_TIMEOUT=5000

# Features Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# Database Configuration
REACT_APP_DATABASE_URL=mongodb://localhost:27017/template-selector-dev

# Authentication
REACT_APP_AUTH_ENABLED=false
REACT_APP_AUTH_PROVIDER=local

# Logging
REACT_APP_LOG_LEVEL=debug
REACT_APP_LOG_TO_CONSOLE=true

# Performance
REACT_APP_CACHE_DURATION=300000
REACT_APP_DEBOUNCE_DELAY=300

# Testing
REACT_APP_TEST_MODE=true
REACT_APP_MOCK_DATA=true
