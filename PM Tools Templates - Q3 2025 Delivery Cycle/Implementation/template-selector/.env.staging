# Staging Environment Configuration
NODE_ENV=staging
REACT_APP_ENVIRONMENT=staging

# API Configuration
REACT_APP_API_URL=https://staging-api-template-selector.example.com
REACT_APP_API_TIMEOUT=10000

# Features Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG_MODE=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# Database Configuration
REACT_APP_DATABASE_URL=mongodb://staging-db.example.com:27017/template-selector-staging

# Authentication
REACT_APP_AUTH_ENABLED=true
REACT_APP_AUTH_PROVIDER=staging-auth

# Logging
REACT_APP_LOG_LEVEL=info
REACT_APP_LOG_TO_CONSOLE=false

# Performance
REACT_APP_CACHE_DURATION=600000
REACT_APP_DEBOUNCE_DELAY=300

# Testing
REACT_APP_TEST_MODE=false
REACT_APP_MOCK_DATA=false

# Monitoring
REACT_APP_SENTRY_DSN=https://staging-sentry-dsn.example.com
REACT_APP_MONITORING_ENABLED=true
