{
  "name": "ai-insights",
  "version": "1.0.0",
  "description": "AI-Powered Project Insights for PM Tools Templates",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:api": "node src/api/server.js",
    "start:prod": "NODE_ENV=production node src/api/server.js",
    "dev": "nodemon src/index.js",
    "dev:api": "nodemon src/api/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest tests/integration.test.js --testTimeout=60000",
    "test:coverage": "jest --coverage",
    "test:quick": "node scripts/run-tests.js quick",
    "test:full": "node scripts/run-tests.js full",
    "test:performance": "node scripts/run-tests.js performance",
    "test:all": "node scripts/run-tests.js all",
    "test:ai-basic": "node scripts/test-ai-basic.js",
    "test:load": "node tests/performance/load-test.js",
    "test:ai-validation": "echo \"AI validation tests would run here\"",
    "test:security": "npm audit --audit-level moderate",
    "train": "node src/ml/train.js",
    "predict": "node src/ml/predict.js",
    "serve": "node src/api/server.js",
    "build": "tsc",
    "build:docker": "docker build -t ai-insights:latest .",
    "deploy:docker": "docker-compose up -d",
    "deploy:prod": "docker-compose -f docker-compose.yml --profile monitoring up -d",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "data:generate": "node src/data/generator.js",
    "health": "curl -f http://localhost:3001/health || exit 1",
    "benchmark": "node scripts/benchmark.js",
    "validate": "npm run lint && npm run test",
    "precommit": "npm run validate",
    "clean": "rm -rf node_modules data/cache/* data/logs/*",
    "reset": "npm run clean && npm install"
  },
  "keywords": [
    "machine-learning",
    "project-management", 
    "ai",
    "insights",
    "risk-prediction",
    "optimization"
  ],
  "author": "PM Tools Templates",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.15.0",
    "express": "^4.22.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.23",
    "moment": "^2.29.4",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "natural": "^6.12.0",
    "sentiment": "^5.0.2",
    "ml-matrix": "^6.10.7",
    "regression": "^2.0.1",
    "node-cache": "^5.1.2",
    "uuid": "^9.0.1",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/cors": "^2.8.17"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
